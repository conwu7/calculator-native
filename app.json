{
  "expo": {
    "name": "Calculator",
    "slug": "calculator-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "primaryColor": "#2f4f4f",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashing.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f4f4f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2f4f4f"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "conwu7.calculator"
    },
    "androidStatusBar": {
      "backgroundColor": "#2f4f4f"
    },
    "androidNavigationBar": {
      "backgroundColor": "#2f4f4f"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
